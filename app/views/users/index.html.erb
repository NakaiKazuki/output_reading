<% provide(:title, 'ユーザー一覧') %>
<div class="container users-index-container">
  <div class="row">
    <div class="col">
      <h1 class="index-title">ユーザー一覧</h1>
      <%= paginate @users %>
      <ul class="users">
        <% @users.each do |user| %>
          <li>
            <%= link_to user.name, user ,class: 'users-index-name' %>
          </li>
        <% end %>
      </ul>
      <%= paginate @users %>
    </div>
  </div>
</div>
